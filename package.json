  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fh1ch/node-bacstack/issues"
  },
  "homepage": "https://github.com/fh1ch/node-bacstack#readme",
  "dependencies": {
    "debug": "^2.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "docdash": "^0.4.0",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0"
